{
  "name": "sagui",
  "version": "0.1.0",
  "description": "Control Panel for Sagui project",
  "author": "La√≠s Tomaz <lais@eokoe.com>",
  "private": true,
  "scripts": {
    "api": "json-server --watch db.json",
    "autoprefixer": "postcss --use autoprefixer cssnano --safe -r dist/css/**/*.css",
    "build:img": "npm run imagemin && npm run icons",
    "build:sass": "npm run scss",
    "build:vendor": "browserify _source/javascript/app/vendor.js | uglifyjs -c warnings=false -m > dist/js/vendor.js",
    "build:vue": "cross-env NODE_ENV=production browserify -g envify -p [ vueify/plugins/extract-css -o dist/css/build.css ] -e _source/javascript/app/application.js | uglifyjs -c warnings=false -m > dist/js/build.js",
    "build:prod": "npm run build:vue && npm run build:vendor && npm run build:sass && npm run autoprefixer && npm run nano && npm run imagemin",
    "cachebust": "hashmark -l 8 dist/**/*.{js,css} public_html/{dir}/{name}-{hash}{ext} | replaceinfiles -s 'dist/*.css' -d '{dir}/{base}'",
    "dev": "npm-run-all --parallel watch:all serve",
    "icons": "svgo -f _source/images/**/*.svg -o dist/img",
    "imagemin": "imagemin _source/images/**/*.{gif,jpg,png} --out-dir=dist/img",
    "lint:es": "eslint --ext .js,.vue _source/javascript/app",
    "lint:sass": "sass-lint -c app/config/.sass-lint.yml '_source/stylesheets/**/*.scss' -v -q -i 'tests/**/*.scss, _source/stylesheets/vendor/**/*.scss'",
    "lint:all": "npm run lint:es && npm run lint:sass",
    "nano": "postcss --use cssnano --safe -r dist/css/**/*.css",
    "prebuild:prod": "npm run lint:all && npm run test:prod",
    "scss": "node-sass _source/stylesheets --output-style expanded --output dist/css --source-map true",
    "serve": "http-server -o -c 1 -a localhost",
    "test": "karma start karma.conf.js",
    "test:prod": "karma start karma.conf.js --single-run",
    "version": "version-changelog CHANGELOG.md && changelog-verify CHANGELOG.md && git add CHANGELOG.md",
    "watch:sass": "onchange '_source/stylesheets/**/*.scss' -- npm run build:sass",
    "watch:vue": "watchify -vd -p browserify-hmr -e _source/javascript/app/application.js -o dist/js/build.js",
    "watch:all": "npm run watch:sass & npm run watch:vue"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify"
    ]
  },
  "browser": {
    "vue": "vue/dist/vue.common.js"
  },
  "dependencies": {
    "admin-lte": "^2.4.0",
    "axios": "^0.16.2",
    "font-awesome-picker": "^1.0.0",
    "jquery": "^3.2.1",
    "raven-js": "^3.18.1",
    "vue": "^2.0.1",
    "vue-observe-visibility": "^0.2.2",
    "vue-router": "^2.7.0",
    "vuex": "^2.3.1",
    "vuex-i18n": "^1.4.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-runtime": "^6.0.0",
    "babelify": "^7.2.0",
    "bootstrap-sass": "^3.3.7",
    "browserify": "^13.1.0",
    "browserify-hmr": "^0.3.1",
    "chai": "^4.0.2",
    "changelog-verify": "^1.0.4",
    "cross-env": "^2.0.0",
    "cssnano": "^3.10.0",
    "envify": "^3.4.1",
    "eslint": "^3.3.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-config-standard": "^5.3.5",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^1.7.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "font-awesome": "^4.7.0",
    "hashmark": "^5.0.0",
    "http-server": "^0.9.0",
    "imagemin-cli": "^3.0.0",
    "json-server": "^0.11.2",
    "karma": "^1.2.0",
    "karma-browserify": "^5.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-nyan-reporter": "^0.2.5",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.26",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.3",
    "npm-run-all": "^2.3.0",
    "onchange": "^3.2.1",
    "phantomjs-prebuilt": "^2.1.3",
    "postcss-cli": "^4.1.0",
    "proxyquireify": "^3.0.1",
    "sass-lint": "^1.10.2",
    "svgo": "^0.7.2",
    "uglify-js": "^2.5.0",
    "version-changelog": "^2.1.0",
    "vueify": "^9.0.0",
    "watchify": "^3.4.0"
  }
}
